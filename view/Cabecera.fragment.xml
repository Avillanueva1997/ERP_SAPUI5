<core:FragmentDefinition
    xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
    xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
    xmlns:tnt="sap.tnt"
	xmlns:core="sap.ui.core">
    
    <Toolbar height="40px" design="Transparent"> 
        <content>
            <Button icon="sap-icon://sys-enter-2" type="Transparent"/>
            <Input width="150px" />            
            <Button icon="sap-icon://close-command-field" type="Transparent"/>
            <Button icon="sap-icon://save" type="Transparent"/>
            <Button icon="sap-icon://sys-back-2" type="Transparent"/>
            <Button icon="sap-icon://system-exit-2" type="Transparent"/>
            <Button icon="sap-icon://sys-cancel-2" type="Transparent"/>
            <ToolbarSpacer/>
            <Button icon="sap-icon://log" press="onPress" ariaLabelledBy="editButtonLabel" type="Transparent"/>
        </content>        
    </Toolbar>
    
</core:FragmentDefinition>
