<mvc:View
	controllerName="sap.ui.su01.controller.LOGISTICA.MK.MK01"
	xmlns:l="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:tnt="sap.tnt"
    xmlns:u="sap.ui.unified"
	xmlns="sap.m">    
    <core:Fragment fragmentName='sap.ui.su01.view.Cabecera' type='XML' />
    <Page
		showHeader="true"
		enableScrolling="false" >
        <customHeader>            
			<Toolbar>                
				<ToolbarSpacer/>
				<Title text="Acreedor Crear: Acceso" level="H2"/>			
                <ToolbarSpacer/>
			</Toolbar>
		</customHeader> 
        <subHeader>
			<Toolbar>
				<Button icon="sap-icon://document" press="onNew" tooltip="Nuevo"/>
				<!--<Button icon="sap-icon://copy" press="onCopy"/>-->
			</Toolbar>
		</subHeader>
        <content>
            <f:SimpleForm
                editable="true"
                layout="ResponsiveGridLayout">
                <f:content>
                    <Label text="Acreedor" id="lblAcreedor"/>
                    <Input maxLength="11" width	="10em" id="iptLifnr" liveChange="validacionN"/>                        
                </f:content>
            </f:SimpleForm>
        </content>
	</Page>
</mvc:View>