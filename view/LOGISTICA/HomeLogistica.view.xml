                             press="" />
               <StandardTile
                             icon="sap-icon://edit"
                             title="Editar Material" />
               <StandardTile
                             icon="sap-icon://product"
                             title="Visualizar Material" />
           </TileContainer> 
       </content>
    </Page>
</mvc:View>