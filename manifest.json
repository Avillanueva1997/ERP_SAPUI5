{
  "_version": "1.1.0",
  "sap.app": {
	"_version": "1.1.0",
	"id": "sap.ui.su01",
	"type": "application",
	"i18n": "i18n/i18n.properties",
	"title": "ERP",
	"description": "{{appDescription}}",
	"applicationVersion": {
	  "version": "1.0.0"
	}
  },
  "sap.ui": {
	"_version": "1.1.0",
	"technology": "UI5",
	"deviceTypes": {
	  "desktop": true,
	  "tablet": true,
	  "phone": true
	},
	"supportedThemes": [
	  "sap_belize"
	]
  },
  "sap.ui5": {
	"_version": "1.1.0",
	"rootView": "sap.ui.su01.view.App",
	"dependencies": {
	  "minUI5Version": "1.30",
	  "libs": {
		"sap.m": {}
	  }
	},
	"models": {
	  "i18n": {
		"type": "sap.ui.model.resource.ResourceModel",
		"settings": {
		  "bundleName": "sap.ui.su01.i18n.i18n"
		}
	  }
	},
      "routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "sap.ui.su01.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide"
			},
			"routes": [
                {
				"pattern": "",
				"name": "login",
				"target": "login"
                },
                {
				"pattern": "home",
				"name": "home",
				"target": "home"
                },
                {
				"pattern": "LOGISTICA/homelogistica",
				"name": "homelogistica",
				"target": "homelogistica"
                }
            ],
			"targets": {
				"login": {
                    "path": "sap.ui.su01.view",
					"viewName": "Login",
                    "viewId" : "viewLogin",
                    "viewLevel" : 1
				},
                "home": {
                    "path": "sap.ui.su01.view",
					"viewName": "Home",
                    "viewId" : "viewHome",
                    "viewLevel" : 2
				},
                "homelogistica": {
                    "path": "sap.ui.su01.view",
					"viewName": "LOGISTICA/HomeLogistica",
                    "viewId" : "viewHomeLogistica",
                    "viewLevel" : 3
				}
			}
		}
  }
}